<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="producerFactory" class="cn.eleven.basic.data.rocketmq.client.producer.ProducerFactory">
        <property name="namesrvAddr" value="127.0.0.1:9876"/>
        <property name="topic" value="produceTopic"/>
        <property name="tags" value="eleven"/>
    </bean>
    <bean id="defaultMQConsumer" class="cn.eleven.basic.data.rocketmq.client.consumer.DefaultMQConsumer">
        <property name="namesrvAddr" value="127.0.0.1:9876"/>
        <property name="topic" value="produceTopic"/>
        <property name="subExpression" value="eleven"/>
    </bean>

</beans>